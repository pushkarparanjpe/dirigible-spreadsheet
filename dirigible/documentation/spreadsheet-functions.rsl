from os.path import dirname, join
import time
from urllib import urlopen

from BeautifulSoup import BeautifulSoup

microsoft_excel_functions_www_page = 'http://office.microsoft.com/en-us/excel-help/list-of-worksheet-functions-by-category-HP005204211.aspx'


class FunctionDef(object):
    def __repr__(self):
        return '<def %s>' % (self.excel_function_name,)
    
    @property
    def category(self):
        getcat = lambda r: mt.Cols['category'][r] and mt.Cols['category'][r] or getcat(r-1)
        return getcat(self._row)


