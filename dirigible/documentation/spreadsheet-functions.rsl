    <postFormulaeUserCode><![CDATA[
TITLE = 'Common Spreadsheet Functions in Dirigible'
UNFINISHED_MARKER = 'Coming soon'
UNFINISHED_LINK = '`' + UNFINISHED_MARKER + '`_'

SUPPORT_TEXT = '''
.. _''' + UNFINISHED_MARKER + ''':
'''

def get_definitions():
    definitions = []
    for cell in mt.Cols['function'].Cells:
        if cell.Value:
            new = FunctionDef()
            new._row = cell.Row
            new.excel_function_name = cell.Value or ''
            new.excel_syntax = mt.Cols['syntax'][cell.Row] or mt.Cols['function'][cell.Row] + '()'
            new.excel_description = mt.Cols['excel description'][cell.Row] or ''
            new.howto_in_resolver = mt.Cols['howto in resolver'][cell.Row]
            new.imports = mt.Cols['imports'][cell.Row]
            new.comments = mt.Cols['comments'][cell.Row]
            definitions.append(new)
    return definitions

def output_title(title, underline, out_file):
    print >> out_file,  title
    print >> out_file,  underline * len(title)
    print >> out_file,  ''
    
def output_header(out_file):
    output_title(TITLE, '=', out_file)
    print >> out_file, SUPPORT_TEXT

def format_syntax(syntax):
    syntax = syntax.replace(' (', '(')
    syntax = syntax.replace('(', '( ')
    syntax = syntax.replace(')', ' )')
    syntax = syntax.replace(',', ', ')
    syntax = '| **' + syntax.replace('(', '**\ (')    
    return syntax

def get_func_desc(syntax, description):
    lines = [
        format_syntax(syntax),
        '',
        r'\ *%s*' % (description,),
    ]
    return lines

def get_python_equiv(imports, howto, comment):
    lines = []

    if imports or howto:
        lines.extend(['::', ''])
        if imports:
            lines.append(' ' + imports)
        if howto:
            indent = ' '
            for h in howto.split('\n'):
                if h == '::':
                    indent = ''
                    lines.append('')
                    continue
                elif h.endswith('::'):
                    lines.append(h)
                else:
                    lines.append(indent + h)
                indent = ' '
        lines.append('')

    if comment:
        lines.append(r'\ *%s*' % (comment,))

    if not (howto or comment):
        lines.append(UNFINISHED_LINK)

    return lines

def get_row(defn):
    return [
        get_func_desc(defn.excel_syntax, defn.excel_description),
        get_python_equiv(defn.imports, defn.howto_in_resolver, defn.comments),
    ]

def get_col_widths(headings, rows):
    col_widths = map(len, headings)
    for row in rows:
        for index, col in enumerate(row):
            for line in col:
                col_widths[index] = max(col_widths[index], len(line))
    return col_widths

def get_row_divider(col_widths):
    table_line = '+'
    for width in col_widths:
        table_line += '-' * width + '+'
    return table_line

def output_row(row, col_widths, out_file):
    for line_parts in izip_longest(*row):
        line = '|'
        for index, line_part in enumerate(line_parts):
            text = line_part if line_part else ''
            spacing = ' ' * (col_widths[index] - len(text))
            line += text + spacing + '|'
        print >> out_file, line
    return line


def output_category(cat, definitions, out_file):
    output_title(cat, '-', out_file)
    out_headings = ['Function', 'Python Equivalent']
    rows = [get_row(defn) for defn in definitions]
    col_widths = get_col_widths(out_headings, rows)
    row_divider = get_row_divider(col_widths)

    print >> out_file, row_divider
    output_row([[h] for h in out_headings], col_widths, out_file)
    print >> out_file, row_divider.replace('-', '=')
    for row in rows:
        output_row(row, col_widths, out_file)
        print >> out_file, row_divider
    print >> out_file,  ''


def output_rst():
    definitions = get_definitions()
    out_file = file(join(dirname(__file__), 'spreadsheet-functions.rst'), 'w')
    try:
        output_header(out_file)
        categories = [c.Value for c in mt.Cols['category'].Cells if c.Value]
        for category in categories[1:]:
            output_category(
                category,
                [d for d in definitions if d.category==category],
                out_file)
    finally:    
        out_file.close()

output_rst()
