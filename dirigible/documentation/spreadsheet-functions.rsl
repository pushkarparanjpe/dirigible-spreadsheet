    <preFormulaeUserCode><![CDATA[mt = workbook['main table']

    
def fetch_syntaxes_from_web():
    main_doc_page = BeautifulSoup(urlopen(microsoft_excel_functions_www_page).read())
    all_links = main_doc_page.findAll('a', href=True)
    
    def find_link_for_formula(formula_name):
        for link in all_links:
            if link.string == formula_name:
                return link['href']

    def get_syntax_from_page(href, formula_name):
        try:
            page = BeautifulSoup(urlopen(href).read())
            elements = page.find('div',{'class':'cdArticleBody'}).find('b',text=formula_name).parent.parent.contents
            return ''.join(e.string for e in elements)
        except:
            return ''

    def get_syntax(name, formula_page):
        print name,
        syntax = get_syntax_from_page(formula_page, cell.Value)
        retries = 3
        while not syntax and retries > 0:
            time.sleep(1)
            retries = retries - 1
            print retries,
        print syntax
        return syntax
    
    for cell in mt.Cols['function'].Cells:
        if cell.Value and cell.Row > 1:
            formula_page = find_link_for_formula(cell.Value)
            mt.Cells[cell.Col, cell.Row].Href = formula_page
            syntax = get_syntax(cell.Value, formula_page)
            if syntax:
                mt.Cells[cell.Col+1, cell.Row].Formula = syntax

b = Button('Fetch Syntaxes from web')
b.Click += fetch_syntaxes_from_web
mt.C2 = b
            
]]></preFormulaeUserCode>
